modules = ["python-3.11", "nodejs-20", "postgresql-16"]
run = "python3 bot.py"
[nix]
channel = "stable-25_05"
packages = ["freetype", "glibcLocales", "postgresql", "semgrep"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Bot & Landing Page"

[[workflows.workflow]]
name = "Bot & Landing Page"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "gunicorn app:app --bind 0.0.0.0:5000 --workers 1 --timeout 120 --preload --log-level info --access-logfile - --error-logfile -"
waitForPort = 5000

[deployment]
deploymentTarget = "vm"
run = ["gunicorn", "app:app", "--bind", "0.0.0.0:5000", "--workers", "1", "--timeout", "120", "--preload", "--log-level", "info", "--access-logfile", "-", "--error-logfile", "-"]
build = ["pip", "install", "-r", "requirements.txt"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8081
externalPort = 8081

[agent]
expertMode = true
integrations = ["flask_stripe:1.0.0", "replitmail:1.0.0"]

